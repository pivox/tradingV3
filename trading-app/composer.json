{
  "name": "trading/app",
  "type": "project",
  "require": {
    "php": ">=8.2",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-pdo": "*",
    "ext-pdo_pgsql": "*",
    "symfony/framework-bundle": "^7.1",
    "symfony/console": "^7.1",
    "symfony/dotenv": "^7.1",
    "symfony/runtime": "^7.1",
    "symfony/http-client": "^7.1",
    "symfony/monolog-bundle": "^3.10",
    "symfony/rate-limiter": "^7.1",
    "symfony/lock": "^7.1",
    "symfony/process": "^7.1",
    "symfony/serializer": "^7.1",
    "symfony/validator": "^7.1",
    "doctrine/orm": "^3.3",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/doctrine-migrations-bundle": "^3.5",
    "doctrine/dbal": "^3.7",
    "react/socket": "^1.12",
    "ratchet/pawl": "^0.4",
    "guzzlehttp/guzzle": "^7.8",
    "ramsey/uuid": "^4.7",
    "brick/math": "^0.11",
    "symfony/yaml": "*",
    "symfony/clock": "*",
    "symfony/twig-bundle": "*"
  },
  "require-dev": {
    "symfony/var-dumper": "^7.1",
    "phpstan/phpstan": "^1.11",
    "phpunit/phpunit": "^11.3",
    "symfony/maker-bundle": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "App\\Tests\\": "tests/"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "extra": {
    "symfony": {
      "allow-contrib": true,
      "require": "7.1.*"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "symfony/runtime": true
    }
  }
}
