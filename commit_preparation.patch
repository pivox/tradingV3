+                $elapsed = $now - $lastRequest;
+                if ($elapsed < self::THROTTLE_SECONDS) {
+                    usleep((int) round((self::THROTTLE_SECONDS - $elapsed) * 1_000_000));
+                    $now = microtime(true);
+                }
+            }
+
+            @file_put_contents($this->throttleStatePath, sprintf('%.6F', $now));
+        } finally {
+            $lock->release();
+        }
